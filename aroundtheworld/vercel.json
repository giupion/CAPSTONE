{
    "version": 2,
    "builds": [
      {
        "src": "server.php",
        "use": "@vercel/php"
      },
      {
        "src": "resources/js/app.js",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "public"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/server.php"
      }
    ]
  }
  