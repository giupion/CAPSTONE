{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "public"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        
        "APP_URL": "https://your-deployment-url.vercel.app",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "sql7.freemysqlhosting.net",
        "DB_PORT": "3306",
